sc_agent_for_finding_value
=> nrel_main_idtf:
	[sc-агент тест] (* <- lang_ru;; *);
	[sc-agent test] (* <- lang_en;; *);
<- abstract_sc_agent;
=> nrel_primary_initiation_condition: (sc_event_add_output_arc => question_initiated);
=> nrel_initiation_condition_and_result: (..sc_agent_for_finding_value_condition => ..sc_agent_for_finding_value_result);
<= nrel_sc_agent_key_sc_elements: 
	{
		question_initiated;
		question;
		question_finding_value
	};
<- key_sc_element:
	...
	(*
	<- sc_description_of_sc_agent_behavior;;
	<= nrel_sc_text_translation:
		...
		(*
		-> rrel_example:
			[Задачей sc-агента является тест.];;
		*);;
	*);
=> nrel_inclusion: ...
	(*
		<- platform_independent_abstract_sc_agent;;
		<= nrel_sc_agent_program: 
		{
			agent_finding_value;
			proc_finding_value
		};;
		-> sc_agent_for_finding_value_scp   /*(* <- active_sc_agent;; *)*/;;
	*);;

..sc_agent_for_finding_value_condition
= [*
	question_w _-> .._question;;
	question_initiated _-> .._question;;
	question _-> .._question;;
	.._question _-> .._parameter;;
*];;

..sc_agent_for_finding_value_result
= [*
	question_w _-> .._question;;
	question_finished _-> .._question;;
	question _-> .._question;;
	.._question _=> nrel_answer:: .._answer;;
	.._question _-> .._parameter;;
*];;
